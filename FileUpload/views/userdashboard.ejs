<script type="text/javascript">

var xmlhttp;
if (window.XMLHttpRequest)
  {// code for IE7+, Firefox, Chrome, Opera, Safari
  xmlhttp=new XMLHttpRequest();
  }
else
  {// code for IE6, IE5
  xmlhttp=new ActiveXObject("Microsoft.XMLHTTP");
  }
xmlhttp.onreadystatechange=function()
  {
  if (xmlhttp.readyState==4 && xmlhttp.status==200)
    {
		//alert(xmlhttp.responseText);
		var user = sessionStorage.getItem('user');
//alert(user); 
document.getElementById('username').innerHTML= "<b>"+user+"<b>";
		var data = xmlhttp.responseText.split(':');
		var dat1 = data[0].replace(']','').replace('[','').split(',');
		var dat2 = data[1].replace(']','').replace('[','').split(',').map(Number);
		//alert(data[0]+data[1]);
		$(function () {
	        $('#container').highcharts({
	            chart: {
	                type: 'spline'
	            },
	            title: {
	                text: 'Monthly hours active'
	            },
	            subtitle: {
	                text: ''
	            },
	            xAxis: {
	                categories: dat1
	            },
	            yAxis: {
	                title: {
	                    text: 'Hours'
	                },
	                labels: {
	                    formatter: function() {
	                        return this.value +'Mb'
	                    }
	                }
	            },
	            tooltip: {
	                crosshairs: true,
	                shared: true
	            },
	            plotOptions: {
	                spline: {
	                    marker: {
	                        radius: 4,
	                        lineColor: '#666666',
	                        lineWidth: 1
	                    }
	                }
	            },
	            series: [{
	                name: 'Hours',
	                data: dat2
	    
	            }]
	        });
	    });
	    
	  //document.getElementById("myDiv").innerHTML=xmlhttp.responseText;
    }
  }
xmlhttp.open("POST","http://localhost:8080/SecurityCloud/csfContoller",true);
//alert(xmlhttp.readyState);
//alert(xmlhttp.status);
xmlhttp.send("dashboard");



</script>
